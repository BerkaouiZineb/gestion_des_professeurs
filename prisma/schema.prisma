generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Professeur {
  id       Int    @id @default(autoincrement())
  nom      String
  prenom   String
  email    String  @unique
  telephone String
  matieres String
  statut   String
  photo    String? // Stocker l'URL de l'image
  password String
}
